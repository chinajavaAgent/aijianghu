<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aigroup.world.mapper.AdminMapper">
    <select id="getAdminByTipId" resultType="com.aigroup.world.entity.Admin">
        SELECT a.* FROM admin a
        INNER JOIN admin_tips at ON a.id = at.admin_id
        WHERE at.tips_id = #{tipId}
        LIMIT 1
    </select>
</mapper> 